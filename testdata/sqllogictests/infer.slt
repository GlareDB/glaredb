# Tests for inferring table functions from file paths.

query I
select count(*) from './testdata/parquet/userdata1.parquet'
----
1000

query IT
select id, "./testdata/parquet/userdata1.parquet".first_name
  from './testdata/parquet/userdata1.parquet'
  order by id
  limit 1
----
1  Amanda


