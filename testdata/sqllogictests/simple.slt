# Simple queries.

query II nosort
select 1, 1 + 1
----
1 2

query III nosort
select * from (values (1, 2, 3), (3, 4, 5))
----
1 2 3
3 4 5

statement ok
create table t1 (a smallint, b smallint)

# Table already exists.
statement error
create table t1 (a smallint, b smallint)

statement ok
insert into t1 values (1, 2), (3, 4), (5, 6)

# Too few values.
statement error
insert into t1 values (1)

# Too many values.
statement error
insert into t1 values (1, 2, 3)

query II nosort
select * from t1 order by a
----
1 2
3 4
5 6

# query II nosort
# select * from t1 order by a desc
# ----
# 5 6
# 3 4
# 1 2

query II
select sum(a), sum(b) from t1
----
9 12

# query II
# select sum(b), sum(a) from t1
# ----
# 12 9

query II
select * from t1 where false;
----

query II
select * from t1 where a >= 3;
----
3 4
5 6
