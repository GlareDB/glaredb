# Basic create table tests

statement ok
create schema create_table_schema;

statement ok
set search_path to create_table_schema;

statement ok
create table t1 (a int, b int);

query II
select * from t1;
----

statement ok
insert into t1 values (1, 2);

query II rowsort
select * from t1
----
1  2

statement ok
insert into t1 values (3, 4), (5, 6);

query II rowsort
select * from t1;
----
1  2
3  4
5  6

