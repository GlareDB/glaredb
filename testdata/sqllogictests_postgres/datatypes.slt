# Test if the datasource supports the different datatypes (and NULLs).

include ./include/conn.slti

# Create an external table that connects to the datatypes table.
statement ok
CREATE EXTERNAL TABLE datatypes
	FROM testconn
	OPTIONS (
		schema = 'public',
		table = 'datatypes'
	);

