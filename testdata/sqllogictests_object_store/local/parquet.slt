# Creating external tables with parquet files.

statement ok
create schema parquet;

statement ok
set search_path = parquet;

statement ok
create connection if not exists local_filesystem_conn for local;

statement ok
create external table userdata1 from local_filesystem_conn options(location='${PWD}/testdata/parquet/userdata1.parquet');

query I
select count(*) from userdata1;
----
1000

query IT
select count(*), gender from userdata1 group by gender order by gender;
----
67   (empty)
482  Female
451  Male
