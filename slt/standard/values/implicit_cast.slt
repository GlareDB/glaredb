# Implicit casting in VALUES to ensure every row has the same types.

query TT
DESCRIBE SELECT * FROM (VALUES (4), (NULL));
----
column1  Int64

query I rowsort
SELECT * FROM (VALUES (4), (NULL));
----
4
NULL

