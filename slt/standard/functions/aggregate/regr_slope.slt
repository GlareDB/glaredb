# REGR_SLOP function

query R
SELECT regr_slope(4, 5);
----
NULL

query R
SELECT regr_slope(a, b)
  FROM (VALUES (4, 5),
               (6, 7)) v(a,b);
----
1

query R
SELECT regr_slope(a, b)
  FROM (VALUES (4, 5),
               (6, 1)) v(a,b);
----
-0.5

query R
SELECT regr_slope(a, b)
  FROM (VALUES (4, 5),
               (6, 7),
               (8, NULL),
               (NULL, 9)) v(a,b);
----
1

query R
SELECT regr_slope(a, b)
  FROM (VALUES (4, 5),
               (4, 5)) v(a,b);
----
