# concat (string) function

query T
select concat();
----
(empty)

query T
select concat('a');
----
a

query T
select concat('a', 'b');
----
ab

query T
select concat(a, b) from (values ('mario', 'luigi'), ('peach', 'yoshi')) v(a, b);
----
marioluigi
peachyoshi

