# DESCRIBE all tpch parquet files

query TT
describe '../submodules/testdata/tpch_sf0.1/customer.parquet'
----
c_custkey     Int32
c_name        Utf8
c_address     Utf8
c_nationkey   Int32
c_phone       Utf8
c_acctbal     Decimal64(15,2)
c_mktsegment  Utf8
c_comment     Utf8

query TT
describe '../submodules/testdata/tpch_sf0.1/lineitem.parquet'
